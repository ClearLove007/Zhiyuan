package zhiyuan.mapper;

import java.util.List;

import org.apache.ibatis.annotations.Param;

import zhiyuan.model.Help;
import zhiyuan.model.LiKe1;
import zhiyuan.model.Result;

public interface LiKe1Mapper {
	
	//1.查询所有学校
	public List<LiKe1> findAll();
	
	//2.查询符合条件的学校资料
	public List<Result> findLiKe1School(@Param("xiancha")int xiancha);
	
	//3查询符合条件的学校的四年位次，线差，系数
	public List<Help> findHelp();
}
