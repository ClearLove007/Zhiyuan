　　function test1() { 
　　　　var number1 = document.getElementById("num1").value; 
　　　　var number2 = document.getElementById("num2").value; 
　　　　if (number1=="" || number2=="") { 
　　　　　　alert(‘请输入内容’); 
　　　　　　return false; 
　　　　} 
　　　　if (!(/(^[1-9]\d*$)/.test(number1))&&!(/(^[1-9]\d*$)/.test(number2))) { 
　　　　　　alert(‘输入的不是正整数’); 
　　　　　　return false; 
　　　　}
		return true;
　　} 
</script> 
